{
  "dependencies": {
    "@thetinyspark/moocaccino-barista": "^2.12.0",
    "http-server": "^14.1.0",
    "typescript": "^4.6.3",
    "webpack": "^5.71.0",
    "webpack-cli": "^4.9.2"
  },
  "name": "moocaccino-livecoding",
  "description": "C'est le dépôt où je pose toutes les bêtises que je fais et dis en livecoding :D",
  "version": "1.0.0",
  "main": "webpack.config.js",
  "devDependencies": {
    "@thetinyspark/texture-packer": "^1.12.0"
  },
  "scripts": {
    "serve": "http-server",
    "build": "tsc && npx webpack --config webpack.config.js",
    "build:serve": "tsc && npx webpack --config webpack.config.js && npm run serve",
    "start": "npm run pack:texture && npm run build && npm run serve",
    "pack:texture": "texture-packer --dir ./assets/sprites/rawl/ --output ./assets/sheets --size 4096 --optimize 1 --debug 0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thetinyspark/moocaccino-livecoding.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/thetinyspark/moocaccino-livecoding/issues"
  },
  "homepage": "https://github.com/thetinyspark/moocaccino-livecoding#readme"
}
